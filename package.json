{
	"name": "vscode-terminal-log",
	"displayName": "Terminal Log",
	"description": "ターミナル出力をファイルにアウトプット",
	"repository": {
		"type": "git",
		"url": "https://github.com/HayatoKudou/vscode-terminal-log"
	},
	"version": "0.0.1",
	"publisher": "devwright",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.terminalLog.pasteLog",
				"title": "Terminal: Log"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"vsce": "^1.57.0"
	}
}